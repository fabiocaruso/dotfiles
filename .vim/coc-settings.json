{
	"rust.clippy_preference": "on",
	"rust-analyzer.inlayHints.typeHints": true,
	"rust-analyzer.inlayHints.chainingHints": true,
	"rust-analyzer.checkOnSave.command": "clippy",
	"coc.preferences.jumpCommand": "vsplit",
	"explorer.file.showHiddenFiles": true,
	"explorer.file.reveal.auto": false,
	"explorer.previewAction.onHover": false,
	"explorer.icon.enableNerdfont": true,
	"explorer.icon.source": "vim-devicons",
	"explorer.file.child.template": "[git | 2] [selection | clip | 1] [indent][icon | 1] [diagnosticError & 1][filename omitCenter 1][modified][readonly] [linkIcon & 1][link growRight 1 omitCenter 5]",
	"explorer.buffer.root.template": "[icon & 1] Open Buffers",
	"explorer.file.root.template": "[icon & 1] Project ([root])",
	"explorer.keyMappings.global": {
		"<cr>": ["expandable?", ["expanded?", "collapse", "expand"], "open"],
		"J": ["normal:5j"],
    "K": ["normal:5k"]
	},
	"explorer.openAction.for.directory": "expandOrCollapse",
	"explorer.openAction.strategy": "sourceWindow",
	"explorer.sources": [
		{
			"name": "bookmark",
			"expand": false
		},
		{
			"name": "buffer",
			"expand": false
		},
		{
			"name": "file",
			"expand": true
		}
	],
	"explorer.quitOnOpen": false,
	"explorer.toggle": false,
	"explorer.autoExpandOptions": ["recursive","uncompact"],
	"explorer.width": 30,
	"explorer.focus": false
}
